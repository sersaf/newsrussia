<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="wps.newsrussia.entities.JournalCovers" table="journal_covers" schema="newsrussia">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="img">
            <column name="img" sql-type="varchar(80)" length="80" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>