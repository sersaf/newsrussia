<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="wps.newsrussia.entities.IssueWps" table="issue_wps" schema="newsrussia">
        <id name="idIssueWps" column="id_issue_wps"/>
        <property name="issueName" column="issue_name"/>
        <property name="issueLastmodify" column="issue_lastmodify"/>
        <property name="issueLang" column="issue_lang"/>
        <property name="issueStatus" column="issue_status"/>
        <property name="issueStart" column="issue_start"/>
        <property name="issuePath" column="issue_path"/>
        <property name="genreIdGenre" column="Genre_id_genre"/>
        <property name="storageIdStorage" column="storage_id_storage"/>
        <property name="issueCover" column="issue_cover"/>
    </class>
</hibernate-mapping>